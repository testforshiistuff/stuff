game:GetService("StarterGui"):SetCore("SendNotification", { 
	Title = "Notice:";
	Text = "Copied Discord Link!";
	Icon = "rbxthumb://type=Asset&id=11782547157&w=150&h=150"})
Duration = 16;
end)

