FPS.Name = "FPS"
FPS.Parent = SectionsFrame
FPS.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
FPS.Position = UDim2.new(0.0500000007, 0, 0.907171309, 0)
FPS.Size = UDim2.new(0, 90, 0, 15)
FPS.Font = Enum.Font.Gotham
FPS.Text = "Game FPS: 00"
FPS.TextColor3 = Color3.fromRGB(255, 255, 255)
FPS.TextSize = 10.000
coroutine.wrap(function()
	while wait() do
	local fps = game.Workspace:GetRealPhysicsFPS()
	FPS.Text = "Game FPS: "..math.floor(fps)
	end
end)()

