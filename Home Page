-- Scripts:

local function BCXGDK_fake_script() -- ProfilePicture.LocalScript 
	local script = Instance.new('LocalScript', ProfilePicture)

	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	
	-- Fetch the thumbnail
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	-- Set the ImageLabel's content to the user thumbnail
	local imageLabel = script.Parent
	imageLabel.Image = content
end
coroutine.wrap(BCXGDK_fake_script)()

coroutine.wrap(function()
while wait() do
Info.Text = "Your Using VHub Simple Edition! More Simple, Additional Scripts And More Updates If I Find A Script"
for i = 1,100 do
	Info.TextTransparency += 0.01
	wait(0.01)

Info.Text = "Did You Know? VHub Is 1 Year Old. VHub Was Created On November 9, 2021 Now Its A Successful Hub!"
for i = 1,100 do
	Info.TextTransparency -= 0.01
	wait(0.01)
end
end)()
