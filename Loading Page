-- Scripts:

local function SEAHYP_fake_script() -- LoadingImage.LocalScript 
	local script = Instance.new('LocalScript', LoadingImage)

	while wait() do
		script.Parent.Rotation = script.Parent.Rotation + -10
	end
end
coroutine.wrap(SEAHYP_fake_script)()

