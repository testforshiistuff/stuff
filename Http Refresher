if Refresher_LOADED and not _G.Refresher_DEBUG == true then
	print("Loader Already Loaded") 
	return
end

pcall(function() getgenv().Refresher_LOADED = true end)

coroutine.wrap(function()
while wait() do
wait(30)

game:GetService("CoreGui").VHubCoreScreen.Name = "MainCore"

game:GetService("StarterGui"):SetCore("SendNotification", { 
	Title = "Hey!";
	Text = "VHub Is Gonna Refresh Https To Function VHub Properly! Might Lag A Bit.";
	Icon = "rbxthumb://type=Asset&id=9408318893&w=150&h=150"})
Duration = 16;

wait(2)
loadstring(game:HttpGet(('https://raw.githubusercontent.com/testforshiistuff/stuff/main/Page%20Author'),false))()
