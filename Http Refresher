loadstring(game:HttpGet(('https://raw.githubusercontent.com/testforshiistuff/stuff/main/Scripts%20Page%204'),false))()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/testforshiistuff/stuff/main/Scripts%20Page%205'),false))()

wait(0.1)
game:GetService("CoreGui").VHubCoreScreen:Destroy()
wait(0.1)
game:GetService("CoreGui").MainCore.Name = "VHubCoreScreen"

game:GetService("StarterGui"):SetCore("SendNotification", { 
	Title = "Https Refreshed!";
	Text = "Successfully Refreshed Https!";
	Icon = "rbxthumb://type=Asset&id=9408318893&w=150&h=150"})
